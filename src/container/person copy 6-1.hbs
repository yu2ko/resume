<div>
  {{#with person}}
    <main>
      <section>
        <div>
          <div>
            <h1 h1>{{name}} </h1>
          </div>

          <div>
            <ul>
              <li>age:{{age}}</li>
              <li>gender:{{gender}}</li>
            </ul>
          </div>

          <br />
          <div>
            <address>
              {{#with address}}
                <span>{{street}},
                  {{city}},
                  {{state}},
                  {{zip}},
                  {{country}}</span>
              {{/with}}
            </address>
          </div>
          <br />
          <hr />
        </div>
      </section>

      <section>
        <h2>Education</h2>
        <div>
          {{#each education}}
            <ul>
              <li> {{degree}}</li>
              <li> {{major}}</li>
              <li> {{university}}</li>
              <li> {{graduationYear}}</li>
            </ul>
          {{/each}}
        </div>
      </section>

      <section>
        <div>
          <h2>Work Experience</h2>
          <ul>
            {{#each workExperience}}
              <li>Company:<strong>{{company}}</strong></li>
              <li> {{title}}</li>
              <li>
                {{startDate}}
                -
                {{endDate}}
              </li>
              {{#each responsibilities}}
                <li> {{this}}</li>
              {{/each}}
              <li>Years founded: {{year_founded}}</li>
              <li>Industry: {{industry}}</li>
              <br />
              <em>Employees:</em>
              {{#each employees}}
                <li>{{name}},
                  {{position}},
                  {{department}}</li>
                <br />
                <strong>Projects:</strong>
                {{#each projects}}
                  <li>{{name}}, {{description}}</li>
                  <li>{{status}}</li>
                  <br />
                  <em>Teams:</em>
                  {{#each teams}}
                    <li>{{team_name}}</li>
                    {{#with team_leader}}
                      <li>{{name}},
                        {{title}},
                        {{email}}</li>
                    {{/with}}
                    <br />
                    <em>Team member:</em>
                    <ul>
                      {{#each team_members}}
                        <li>{{name}},
                          {{title}},
                          {{email}}</li>
                        {{#each skills}}
                          <li>{{this}}</li>
                        {{/each}}
                        <br />
                        <strong>Projects:</strong>
                        {{#each projects}}
                          <li>{{name}}</li>
                          <li>{{description}}</li>
                          {{#each technologies}}
                            <li> {{this}}</li>
                          {{/each}}
                          <br />
                          <em>Team members:</em>
                          {{#each team_members}}
                            <li>{{name}}, {{title}}</li>
                          {{/each}}
                        {{/each}}
                      {{/each}}
                    </ul>
                  {{/each}}
                {{/each}}
              {{/each}}
            {{/each}}
          </ul>
        </div>
      </section>
    </main>
  {{/with}}
</div>